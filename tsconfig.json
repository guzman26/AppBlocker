{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": false,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["esnext"],
    "moduleResolution": "bundler",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext"
  },
  "include": [
    "src",
    "index.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
