{
  "expo": {
    "name": "AppBlocker",
    "slug": "appblocker",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "appblocker",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "platforms": [
      "ios"
    ],
    "plugins": [
      "./plugins/withScreenTimeManager.js",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "16.0"
          }
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.appblocker.mobile",
      "supportsTablet": false,
      "usesIcloudStorage": false,
      "entitlements": {
        "com.apple.developer.family-controls": true
      },
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Necesitamos acceder a tu biblioteca para mostrar Ã­conos personalizados.",
        "NSUserTrackingUsageDescription": "Se utiliza para mejorar tu experiencia dentro de AppBlocker.",
        "NSFamilyControlsUsageDescription": "Necesitamos acceder a Screen Time para ayudarte a bloquear aplicaciones y mantener el enfoque."
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.appblocker.mobile"
    }
  }
}
